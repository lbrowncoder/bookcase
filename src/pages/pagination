import React from 'react';

const bookReturn = ({bookresults, loading}) => {
    if(loading) {
        return <h2>loading....</h2>;
    }

return (
     <ul className="list-group mb-4">
        {bookresults.map(bookresults => (
            <li key={bookresults.id} className='list-group-item'>
        {bookresults.title}
    </li>
))}
<ul/>
);
};

export defualt bookReturn